<UserControl x:Class="Todo.Wpf.View.MenuView"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:local="clr-namespace:Todo.Wpf.View"
			 xmlns:loc="clr-namespace:Todo.Client.Localisation;assembly=Todo.Client.Localisation"
			 mc:Ignorable="d"
			 d:DesignHeight="450" d:DesignWidth="800">
	<DockPanel HorizontalAlignment="Center" VerticalAlignment="Center">
		<StackPanel>
			<Button Content="Notes" Margin="5" Padding="10,5" Command="{Binding NavigateToNotes}" />
			<Button Content="TodoItems" Margin="5" Padding="10,5" Command="{Binding NavigateToTodoItems}" />
		</StackPanel>
	</DockPanel>
</UserControl>
